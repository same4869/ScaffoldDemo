}
#kx-tgjp-app,
#djjd-app,
#jrrd-app,
#dcjh-app,
#yw-list-app {
    .card {
        width: 100%;
        background: #ffffff;
        border-radius: 0.26667rem;
        padding: 0.32000rem;
        height: 100%;
        .card-header {
            width: 100%;
            display: flex;
            align-items: center;
            justify-content: space-between;
            > div:first-child {
                font-family: PingFang SC;
                font-size: 0.42667rem;
                font-weight: 500;
                color: #222222;
            }
            > div:last-child {
                font-family: PingFang SC;
                font-size: 0.32000rem;
                font-weight: 400;
                color: #999999;
                display: flex;
                align-items: center;
                > img {
                    width: 0.18667rem;
                    margin-left: 0.10667rem;
                }
            }
        }
    }
    .pagination {
        display: flex;
        justify-content: center;
    }
    .swiper-pagination {
        background: rgba(0, 0, 0, 0.2);
        display: flex;
        border-radius: 0.53333rem;
        position: absolute;
        bottom: 0.16000rem;
        .swiper-pagination-bullet {
            width: 0.21333rem;
            height: 0.08000rem;
            border-radius: 0.53333rem;
            margin: unset;
            background: unset;
        }
        .swiper-pagination-bullet-active {
            background: #ec3529;
        }
    }
}
