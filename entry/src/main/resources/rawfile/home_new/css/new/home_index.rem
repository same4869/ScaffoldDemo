.main-container {
    width: 100%;
    position: relative;
    padding-top: 0.32000rem;
}
.gradient-bg {
    position: absolute;
    top: 0;
    width: 100%;
    height: 10.48000rem;
    background: linear-gradient(179.89deg, #ffffff 38.95%, #f2f4f7 99.9%);
}
.ellipsis {
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}
.ellipsis2l {
    width: 100%;
    overflow: hidden;
    word-break: break-all;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    white-space: normal;
}
.full-card {
    height: 100%;
    width: 100%;
    display: flex;
    flex-direction: column;
}
.loading {
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    text-align: center;
    .img {
        width: 0.64000rem;
        height: 0.64000rem;
        animation: rotate 2s linear infinite;
    }
    .text {
        font-family: PingFang SC;
        font-size: 0.32000rem;
        font-weight: 400;
        line-height: 0.45333rem;
        color: #999999;
    }
}
@keyframes rotate {
    from {
        transform: rotate(0);
    }
    to {
        transform: rotate(360deg);
    }
}
.stock-tag {
    display: inline-flex;
    align-items: center;
    font-size: 0.32000rem;
    font-weight: 400;
    padding: 0.05333rem 0.10667rem;
    background: #f8f9fb;
    .stock-name {
        font-family: PingFang SC;
        line-height: 0.45333rem;
        margin-right: 0.05333rem;
    }
    .rate {
        color: #10a361;
        font-family: DINPro;
        line-height: 0.40000rem;
    }
    .is-up {
        color: #ec3529;
    }
}
.stock-tag + .stock-tag {
    margin-left: 0.21333rem;
}

.kx-tgjp {
    position: relative;
    z-index: 1;
    padding: 0 0.26667rem;
    margin-top: 0.21333rem;
    display: flex;
    .gradient-border {
        flex: 1;
        width: 4.61333rem;
        border-radius: 0.26667rem;
