            height: 100%;
        }
        .kx-header {
            display: flex;
            align-items: center;
            justify-content: space-between;
            .name {
                > img {
                    width: 0.93333rem;
                    display: block;
                }
            }
            .time {
                font-family: HarmonyOS Sans SC;
                font-size: 0.32000rem;
                font-weight: 400;
                line-height: 0.37333rem;
                color: #999999;
                display: flex;
                align-items: center;
                > img {
                    width: 0.18667rem;
                    margin-left: 0.05333rem;
                }
            }
        }
        .content {
            margin-top: 0.10667rem;
            font-family: PingFang SC;
            font-size: 0.37333rem;
            font-weight: 400;
            line-height: 0.53333rem;
            color: #222222;
        }
        .stock-content {
            width: 100%;
            margin-top: 0.21333rem;
            margin-bottom: 0.21333rem;
            white-space: nowrap;
            overflow-x: auto;
        }
        .plate-content {
            width: 100%;
            margin-top: 0.21333rem;
            margin-bottom: 0.32000rem;
            white-space: nowrap;
            overflow-x: auto;
            .plate-tag {
                color: #327ae5;
                font-family: PingFang SC;
                font-size: 0.32000rem;
                font-weight: 400;
                line-height: 0.45333rem;
            }
            .plate-tag + .plate-tag {
                margin-left: 0.21333rem;
            }
        }
        .share-btn {
            font-family: PingFang SC;
            font-size: 0.32000rem;
            font-weight: 400;
            line-height: 0.45333rem;
            margin-top: 0.21333rem;
            display: flex;
            align-items: center;
            color: #999999;
            margin-bottom: 0.32000rem;
            > img {
                width: 0.37333rem;
                margin-right: 0.05333rem;
            }
        }
    }
    .tgjp {
        flex: none;
        position: relative;
        width: 4.61333rem;
        margin-left: 0.24000rem;
        #swiper-container-tgjp {
            overflow: hidden;
            width: 100%;
            height: 3.01333rem;
        }
        .slide-content {
            border-radius: 0.26667rem;
            overflow: hidden;
            display: flex;
            flex-direction: column;
            justify-content: space-between;
            height: 100%;
            position: relative;
        }
        .tgjp-card {
            padding: 0.32000rem;
            .tgjp-header {
                display: flex;
                align-items: center;
                justify-content: space-between;
                .left {
                    display: flex;
                    align-items: center;
                    font-family: PingFang SC;
                    font-size: 0.37333rem;
                    font-weight: 500;
                    line-height: 0.53333rem;
                    color: #222222;
                }
                .avatar {
                    width: 0.48000rem;
                    height: 0.48000rem;
                    margin-right: 0.10667rem;
                    border-radius: 50%;
                }
                .icon {
                    margin-left: 0.05333rem;
                    width: 0.42667rem;
                    height: 0.42667rem;
                }
            }
            .content {
                margin-top: 0.10667rem;
                font-family: PingFang SC;
                font-size: 0.37333rem;
                font-weight: 400;
                line-height: 0.53333rem;
            }
            .people {
                font-family: PingFang SC;
                font-size: 0.32000rem;
                font-weight: 400;
                line-height: 0.45333rem;
                color: rgba(153, 153, 153, 1);
                margin-top: 0.21333rem;
            }
            .hf-tag,
            .yy-tag {
                font-family: PingFang SC;
                font-size: 0.32000rem;
                font-weight: 400;
                line-height: 0.45333rem;
                padding: 0 0.10667rem;
            }
            .hf-tag {
                color: #cf723c;
                background: rgba(207, 114, 60, 0.08);
            }
            .yy-tag {
                color: #327ae5;
                background: rgba(50, 122, 229, 0.08);
            }
        }
        .bg {
            width: 100%;
            height: 100%;
            display: block;
            position: absolute;
            left: 0;
            top: 0;
            z-index: -1;
        }
        .zb-card {
            position: relative;
            border-radius: 0.26667rem;
            overflow: hidden;
            .sub {
                position: absolute;
                z-index: 1;
                left: 0;
                top: 0;
                display: flex;
                align-items: center;
                background: #00000099;
                padding-right: 0.16000rem;
                border-bottom-right-radius: 0.26667rem;
                height: 0.50667rem;
                padding-left: 0.96000rem;
                > img {
                    height: 0.61333rem;
                    position: absolute;
                    left: 0;
                    top: 0;
                }
                .time {
                    font-family: HarmonyOS Sans SC;
                    font-size: 0.32000rem;
                    font-weight: 400;
                    line-height: 0.37333rem;
                    color: #ffffff;
                    margin-left: 0.16000rem;
                }
                .count {
                    font-family: HarmonyOS Sans SC;
                    font-size: 0.32000rem;
                    font-weight: 400;
                    line-height: 0.37333rem;
                    color: #ffffff;
                    margin-left: 0.16000rem;
                }
            }
            .living {
                padding-left: 1.65333rem;
            }
        }

        .rdzx-card {
            padding: 0.32000rem;
            /* background: #f8f9fb;
            border-radius: 0.26667rem; */
            position: relative;
            z-index: 1;
            .card-title {
                display: flex;
                align-items: center;
                font-family: PingFang SC;
                font-size: 0.37333rem;
                font-weight: 500;
                color: #222222;
                margin-left: -0.05333rem;
                > img {
                    width: 0.48000rem;
                    height: 0.48000rem;
                    margin-right: 0.10667rem;
                }
            }
            .title {
                font-family: PingFang SC;
                font-size: 0.37333rem;
                font-weight: 400;
                line-height: 0.53333rem;
                margin-top: 0.10667rem;
                color: #222222;
            }
            .content {
                font-family: PingFang SC;
                font-size: 0.37333rem;
                font-weight: 400;
                margin-top: 0.10667rem;
                color: #666666;
            }
        }
